#include <stdio.h>
int i,j,n,t;
int main()
{
	while (scanf("%d",&n)!=EOF)
	{
		t++;
		if (t>1)
		{
			printf("\n");
		}
		if (n>=1 && n<=9)
		{
			for (i=1;i<=n;i++)
			{
				for (j=1;j<i;j++)
				{
					printf("%d*%d=%-2d ",j,i,j*i);
				}
				printf("%d*%d=%-2d\n",j,i,j*i);
			}
		}
	}
	return 0;
}
