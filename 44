#include <stdio.h>
int a[100],max,min,i,N;
int main()
{
	scanf("%d",&N);
	for (i=0;i<N;i++)
	{
		scanf("%d",&a[i]);
	}
	max=a[0];
	for (i=1;i<N;i++)
	{
		if (a[i]>max)
		{
			max=a[i];
		}
	}
	min=a[0];
	for (i=1;i<N;i++)
	{
		if (a[i]<min)
		{
			min=a[i];
		}
	}
	printf("max=%d, min=%d",max,min);
	return 0;
}
