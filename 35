#include <stdio.h>
int a,b,c,d,e,f,n;
int main()
{
	scanf("%d",&a);
	if (0<a && a<10)
	{
		n=1;
		printf("%d\n%d",n,a);
	}
	else if (9<a && a<100)
	{
		n=2;
		b=a%10;
		c=a/10;
		printf("%d\n%d%d",n,b,c);
	}
	else if (99<a && a<1000)
	{
		n=3;
		b=a%10;
		c=a/10%10;
		d=a/100;
		printf("%d\n%d%d%d",n,b,c,d);
	}
	else if (999<a && a<10000)
	{
		n=4;
		b=a%10;
		c=a/10%10;
		d=a/100%10;
		e=a/1000;
		printf("%d\n%d%d%d%d",n,b,c,d,e);
	}
	else if (9999<a && a<100000)
	{
		n=5;
		b=a%10;
		c=a/10%10;
		d=a/100%10;
		e=a/1000%10;
		f=a/10000;
		printf("%d\n%d%d%d%d%d",n,b,c,d,e,f);
	}
	return 0;
}
