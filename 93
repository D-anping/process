#include <stdio.h>
int a[19],i,n,m;
int main()
{
	while (scanf("%d",&n)!=EOF)
	{
		if (n>0 && n<20)
		{
			for (i=0;i<n;i++)
			{
				scanf("%d",&a[i]);
			}
			scanf("%d",&m);
			for (i=0;i<n;i++)
			{
				if (m==a[i])
				{
					printf("%d\n",i);
					break;
				}
			}
			if (i==n)
			{
				printf("No\n");
			}
		}
	}
	return 0;
}
